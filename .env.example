# Lumos Environment Variables
# Copy this file to .env and fill in your values

# AI Provider Configuration
# Required: At least one AI provider API key must be configured
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Optional: OpenAI Organization ID (for organization-level usage tracking)
OPENAI_ORG_ID=your_openai_organization_id_here

# Research APIs (optional)
STACKOVERFLOW_API_KEY=your_stackoverflow_api_key_here
GITHUB_TOKEN=your_github_token_here

# Debug settings
LUMOS_DEBUG=false
LUMOS_LOG_LEVEL=info

# Cache settings
LUMOS_CACHE_DIR=.lumos/cache
